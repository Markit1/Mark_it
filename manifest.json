{
  "manifest_version": 2,
	"name": "Mark_it!",
    "version": "1.0",
    "description": "Menu contextual al seleccionar",
    "browser_action": {
        "default_icon": "images/editable.png",
        "default_popup":"popup.html",
        "default_title": "Mark it!"
    },
    "permissions": [
    "contextMenus"
	],
  "icons": {
        "16": "images/editable.png"
  },
  "background": {
    "scripts": ["Scripts/main.js"]
  }
}
